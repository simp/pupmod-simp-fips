{
  "name": "simp-fips",
  "version": "0.5.0",
  "author": "SIMP Team",
  "summary": "A SIMP module for managing FIPS",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-fips",
  "project_page": "https://github.com/simp/pupmod-simp-fips",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "fips"
  ],
  "dependencies": [
    {
      "name": "simp/simplib",
      "version_requirement": ">= 4.7.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 6.18.0 < 8.0.0"
    },
    {
      "name": "herculesteam/augeasproviders_core",
      "version_requirement": ">= 2.1.1 < 3.0.0"
    },
    {
      "name": "herculesteam/augeasproviders_grub",
      "version_requirement": ">= 2.3.1 < 4.0.0"
    }
  ],
  "simp": {
    "optional_dependencies": [
      {
        "name": "simp/crypto_policy",
        "version_requirement": ">= 0.1.4 < 2.0.0"
      }
    ]
  },
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "8",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "8",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "8",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.22.1 < 8.0.0"
    }
  ]
}
